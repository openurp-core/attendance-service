    <artifactId>openurp-ws-services-teach-parent</artifactId>
    <version>0.0.2</version>
  </parent>
  <artifactId>openurp-ws-services-teach-attendance</artifactId>
  <packaging>jar</packaging>
  <name>OpenURP Teach Attendance Service</name>
  
  <properties>
